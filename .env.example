# Copy to `.env` and fill with real credentials for local development
# Example Postgres URL: postgres://user:password@localhost:5432/dbname?schema=public
DATABASE_URL="postgresql://postgres:password@localhost:5432/100xcfd?schema=public"

# Redis connection used by services
REDIS_URL="redis://localhost:6379"

# JWT secret for signing tokens (replace with a secure random value in production)
JWT_SECRET="replace-with-a-strong-secret"

# Node environment
NODE_ENV=development
